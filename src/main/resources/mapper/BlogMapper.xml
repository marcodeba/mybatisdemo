<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="marco.springboot.mybatisdemo.demo.BlogMapper">
    <select id="selectBlog" resultType="marco.springboot.mybatisdemo.demo.Blog">
        select * from Blog where id = #{id}
    </select>
</mapper>